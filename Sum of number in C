#include<stdio.h>    
int main(){    
int x=0,y=0,result=0;  
  
printf("enter 1st number:");  
scanf("%d",&x);  
printf("enter 2nd number:");  
scanf("%d",&y);  
  
result=x+y;  
printf("1st number is:%d",x);
printf("2nd number is:%d",y);
printf("sum of the two numbers: %d + %d = %d ",x,y,result);  
  
return 0;  
}    
