#include<stdio.h>    
int main(){    
int x=0,y=0,z=0,result=0;  
  
printf("enter first number:");  
scanf("%d",&x);  
printf("enter second number:");  
scanf("%d",&y);  
printf("enter third number:"); 
scanf("%d",&z);
  
result=x+y+z;  
printf("sum of 3 numbers:%d ",result);  
  
return 0;  
}    
