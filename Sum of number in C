#include<stdio.h>    
int main(){    
int x,y,z,result=x*y*z;  
  
printf("enter first number :x");  
scanf("%d",&x);  
printf("enter second number:y");  
scanf("%d",&y); 
printf("Enter third number:z");
scanf("%d", &z);
  
result=x*y*z;  
printf(" multiplication of 3 numbers:%d ",result);  
  
return 0;  
}    
