#include<stdio.h>    
int main(){    
int x=0,y=0,z=0,result=0 ,a=0;  
  
printf("enter first number:");  
scanf("%d",&x);  
printf("enter second number:");  
scanf("%d",&y); 
printf("Enter third number: ");
scanf("%d", &z);
printf("Enter fourth number: ");
scanf("%d", &a);
  
result=x+y+z+a;  
printf("sum of 4 numbers:%d ",result);  
  
return 0;  
}    
